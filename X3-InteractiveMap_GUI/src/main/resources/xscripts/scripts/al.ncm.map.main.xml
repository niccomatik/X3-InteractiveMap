<script>
<name>al.ncm.map.main</name>
<version>0</version>
<engineversion>52</engineversion>
<description>Description</description>
<arguments>
<argument index="1" name="al.PluginID" type="Var/String" desc="AL Plugin ID" />
<argument index="2" name="al.Event" type="Var/String" desc="AL Event" />
</arguments>
<sourceplaintext>
<![CDATA[
  $AL.STATE.VERSION = 0
  $AL.STATE.ENABLED = 1
  
  $al.State = get global variable: name=$al.PluginID
  if not $al.State
    $al.State = array alloc: size=2
    $Version = get script version
    $al.State[$AL.STATE.VERSION] = $Version
    $al.State[$AL.STATE.ENABLED] = [TRUE]
    set global variable: name=$al.PluginID value=$al.State
  end
  
  if $al.Event == 'init' OR $al.Event == 'reinit'
    $Page.ID = get global variable: name='plugin.ncm.map'
    $al.PluginDescription = sprintf: pageid=$al.PluginID textid=8, null, null, null, null, null
    al engine: set plugin $al.PluginID description to $al.PluginDescription
    al engine: set plugin $al.PluginID timer interval to 1 s
@   = [THIS] -> call script al.ncm.map.init :  AL Plugin ID=$al.PluginID  AL State Data=$al.State
    
  else if $al.Event == 'start'
    $al.State[$AL.STATE.ENABLED] = [TRUE]
@   = [THIS] -> call script al.ncm.map.start :  Al Plugin ID=$al.PluginID  AL State Data=$al.State
    
  else if $al.Event == 'stop'
    $al.State[$AL.STATE.ENABLED] = [FALSE]
@   = [THIS] -> call script al.ncm.map.stop :  AL Plugin ID=$al.PluginID  AL State Data=$al.State
    
  else if $al.Event == 'isenabled'
    $al.Retval = $al.State[$AL.STATE.ENABLED]
    return $al.Retval
    
  else if $al.Event == 'timer'
    $enabled = $al.State[$AL.STATE.ENABLED]
    skip if [FALSE] == $enabled
@     = [THIS] -> call script al.ncm.map.timer.fast :  AL Plugin ID=$al.PluginID  AL State Data=$al.State
    
    
  end
  
  
  
  
  
  
  
  
  
  
  
  return null
]]>
</sourceplaintext>
